#include<iostream>

int main() {
    // Declare and initialize variables
    int a = 10;
    int b = 20;
    int c = 30;

    // Add a and b, store the result in d
    int d = a + b;
    std::cout << d << std::endl;

    // Subtract c from d, store the result in e
    int e = d - c;
    std::cout << e << std::endl;

    return 0;
}
